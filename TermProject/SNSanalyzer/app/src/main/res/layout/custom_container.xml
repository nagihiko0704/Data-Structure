<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/custom_rl"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/custom_title_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="1dp"
        android:layout_marginLeft="20dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:text="기본 정보"
        android:textSize="25dp"
        android:textColor="#FFFFFF"
        android:fontFamily="@font/font_nanum_square_round_eb"
        android:background="@drawable/bg_round_rec_blue"/>

    <RelativeLayout
        android:id="@+id/custom_container_rl"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/bg_round_rec_gray">

    </RelativeLayout>

</RelativeLayout>